name: Module CI

on:
  push:
    branches: [ "master" ]
  pull_request:
    branches: [ "master" ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v3

    - name: configure
      run: ./configure

    - name: Install dependencies
      run: make

    - name: Run check
      run: make check

    - name: Run distcheck
      run: make distche
$GNUMAKE -f <ndk>/build/core/build-local.mk
<parameters>
$ cd <project>
$ <ndk>/ndk-
  
  $ ndk-build clean

  rm: fts_read: No such file or directory 
  $ ndk-build NDK_DEBUG=1
  
